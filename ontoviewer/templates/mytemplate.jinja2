{% extends "layout.jinja2" %}

{% block  more_scripts %}

<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
 
{% endblock  more_scripts %}

{% block content %}
<div id="resultat" >YES </div>
{% endblock content %}

{% block comp_script %}

<script>
	var query =  "select ?s where {?s ?p ?o } LIMIT 5";
	var url = "http://localhost:8080/rdf4j-server/repositories/hcao?query=" + encodeURIComponent(query);
	$.getJSON(url).success(function(data) {
		$('#resultat').html("<p>" + data.results.bindings[0].libelle.value + "</p>");
	})

 </script>
 
{% endblock comp_script %}